Grammar

    0 $accept: prog $end

    1 prog: listainst

    2 listainst: instr listainst
    3          | %empty

    4 instr: declaracion
    5      | asignacion
    6      | incremento
    7      | decrementar
    8      | aumentavalor
    9      | disminuyevalor
   10      | multiplicavalor
   11      | dividevalor
   12      | modulavalor
   13      | comp
   14      | iterativa_while
   15      | iterativa_do
   16      | iterativa_for
   17      | CONTINUE
   18      | RETURN
   19      | BREAK
   20      | PASS
   21      | condicional
   22      | imprimir
   23      | leer
   24      | comentario
   25      | funcion

   26 comentario: comentarioSimple
   27           | comentarioComplejo

   28 comentarioSimple: COMENTARIOSIMPLE

   29 comentarioComplejo: COMENTARIOCOMPLEJO

   30 imprimir: PRINT '(' expr ')'

   31 leer: SCAN '(' expr ')'

   32 iterativa_do: DO bloque WHILE '(' comp ')'

   33 iterativa_for: FOR '(' asignacion ';' comp ';' asignacion ')' bloque

   34 iterativa_while: WHILE '(' comp ')' bloque else

   35 @1: %empty

   36 $@2: %empty

   37 condicional: IF '(' comp ')' @1 bloque $@2 else

   38 bloque: '{' listainst '}'

   39 declaracion: identificador ID

   40 @3: %empty

   41 declaracion: CONST identificador ID @3 '=' expresion

   42 asignacion: ID '=' expresion

   43 incremento: ID INCREMENTO

   44 decrementar: ID DECREMENTO

   45 aumentavalor: ID AUMENTO expresion

   46 disminuyevalor: ID DISMINUCION expresion

   47 multiplicavalor: ID MULTI expresion

   48 dividevalor: ID DIVI expresion

   49 modulavalor: ID MOD expresion

   50 identificador: INT
   51              | FLOAT
   52              | DOUBLE
   53              | CHAR
   54              | LONG
   55              | VOID
   56              | BOOL
   57              | SHORT

   58 expresion: expr
   59          | comp

   60 expr: expr '+' term
   61     | expr '-' term
   62     | term

   63 term: term op factor
   64     | factor

   65 op: '*'
   66   | '/'
   67   | BITSIZ
   68   | BITSDE
   69   | '%'

   70 factor: NUM
   71       | '(' expr ')'
   72       | ID
   73       | VERDAD
   74       | FALSO

   75 comp: expr '>' '=' expr
   76     | expr '>' expr
   77     | expr '<' expr
   78     | expr '<' '=' expr
   79     | expr IGUALDAD expr
   80     | expr DESIGUALDAD expr
   81     | expr AND expr
   82     | expr OR expr
   83     | expr NAND expr
   84     | expr XOR expr
   85     | expr NOR expr

   86 else: ELSE bloque

   87 @4: %empty

   88 $@5: %empty

   89 else: ELIF '(' comp ')' @4 bloque $@5 else
   90     | %empty

   91 funcion: DE_TIPO '(' identificador ')'
   92        | NEGACION '(' expr ')'



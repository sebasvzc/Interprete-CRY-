Grammar

    0 $accept: prog $end

    1 prog: listainst

    2 listainst: instr listainst
    3          | %empty

    4 instr: declaracion
    5      | asignacion
    6      | incremento
    7      | decrementar
    8      | aumentavalor
    9      | disminuyevalor
   10      | multiplicavalor
   11      | dividevalor
   12      | modulavalor
   13      | comp
   14      | iterativa_while
   15      | iterativa_do
   16      | iterativa_for
   17      | CONTINUE
   18      | RETURN
   19      | BREAK
   20      | PASS
   21      | condicional
   22      | imprimir
   23      | leer
   24      | comentario
   25      | funcion

   26 comentario: comentarioSimple
   27           | comentarioComplejo

   28 comentarioSimple: COMENTARIOSIMPLE

   29 comentarioComplejo: COMENTARIOCOMPLEJO

   30 $@1: %empty

   31 imprimir: PRINT '(' expresion $@1 ')'

   32 leer: SCAN '(' expr ')'

   33 iterativa_do: DO bloque WHILE '(' comp ')'

   34 iterativa_for: FOR '(' asignacion ';' comp ';' asignacion ')' bloque

   35 @2: %empty

   36 @3: %empty

   37 @4: %empty

   38 iterativa_while: WHILE '(' @2 comp @3 ')' bloque @4

   39 @5: %empty

   40 $@6: %empty

   41 condicional: IF '(' comp ')' @5 bloque $@6 else

   42 bloque: '{' listainst '}'

   43 @7: %empty

   44 declaracion: identificador ID @7

   45 @8: %empty

   46 declaracion: CONST identificador ID @8 '=' expresion

   47 @9: %empty

   48 asignacion: ID @9 '=' expresion

   49 incremento: ID INCREMENTO

   50 decrementar: ID DECREMENTO

   51 aumentavalor: ID AUMENTO expresion

   52 disminuyevalor: ID DISMINUCION expresion

   53 multiplicavalor: ID MULTI expresion

   54 dividevalor: ID DIVI expresion

   55 modulavalor: ID MOD expresion

   56 identificador: INT
   57              | FLOAT
   58              | DOUBLE
   59              | CHAR
   60              | LONG
   61              | VOID
   62              | BOOL
   63              | SHORT

   64 expresion: expr
   65          | comp

   66 expr: expr '+' term
   67     | expr '-' term
   68     | term

   69 term: term op factor
   70     | factor

   71 op: '*'
   72   | '/'
   73   | BITSIZ
   74   | BITSDE
   75   | '%'

   76 factor: NUM
   77       | '(' expr ')'
   78       | ID
   79       | VERDAD
   80       | FALSO

   81 comp: expr '>' '=' expr
   82     | expr '>' expr
   83     | expr '<' expr
   84     | expr '<' '=' expr
   85     | expr IGUALDAD expr
   86     | expr DESIGUALDAD expr
   87     | expr AND expr
   88     | expr OR expr
   89     | expr NAND expr
   90     | expr XOR expr
   91     | expr NOR expr

   92 else: ELSE bloque

   93 @10: %empty

   94 $@11: %empty

   95 else: ELIF '(' comp ')' @10 bloque $@11 else
   96     | %empty

   97 funcion: DE_TIPO '(' identificador ')'
   98        | NEGACION '(' expr ')'


